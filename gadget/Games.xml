  function new_rankings() {
	var yr = document.getElementById("year_dd").value;
	if (yr <= 2010) {
		var url = "https://sites.google.com/site/swaermodel/GameList_2007-10.json?revision=3";
	} else if (yr <= 2014) {
		var url = "https://sites.google.com/site/swaermodel/GameList_2011-14.json?revision=3";
	} else {
		var url = "https://sites.google.com/site/swaermodel/GameList_2015-17.json?revision=3";
	}
	var wk = document.getElementsByClassName("btn active")[0].innerHTML;
	
	
	bring_ranks(url,yr,wk);
	document.getElementById("tbl-heading").innerText = 'Week ' + wk + ' of ' + yr;
  };

  function bring_ranks(url,yr,wk) {
    var params = {};  
    params[gadgets.io.RequestParameters.CONTENT_TYPE] = gadgets.io.ContentType.JSON;
    gadgets.io.makeRequest(url, function(obj) {response(obj,wk,yr);}, params);
  };

  function response(obj,wk_sel,yr_sel) {
    	               
    var yr = yr_sel;
    var wk = wk_sel-1;
    var responseText = obj.data;

	var html = "<table id='rank'>";
	html += '<thead><tr>';
	html += '<td>Date</td>';
	html += '<td class="filter">Div1</td>';
	html += '<td class="filter">Div2</td>';
	html += '<td></td>';
	html += '<td>SWAER Favorite</td>';
	html += '<td>Spread</td>';
	html += '<td>Final</td>';
	html += '</tr></thead><tbody>';

	for (var i in responseText[yr][wk].Tm_Code) {
		var tm = responseText[yr][wk];
		html += '<tr>';
		html += '<td rowspan=2>' + tm.Q_date[i] + '</td>';
		html += '<td class="filter">' + tm.Q_Opp_Div[i] + '</td>';
		html += '<td class="filter">' + tm.Tm_Div[i] + '</td>';
		html += "<td rowspan=2><img height=50 src='https://github.com/ajreinhard/SWAER/raw/master/helmets/" + tm.Tm_Helm[i]+ ".png'>";
		html += "<img height=50 src='https://github.com/ajreinhard/SWAER/raw/master/helmets/" + tm.Opp_Helm[i]+ ".png'></td>";
		html += '<td colspan=3>#' + tm.Ovr_Rank[i];
		html += " <a href=https://sites.google.com/site/swaermodel/teams/" + tm.Tm_Code[i] + ">" + tm.Tm[i] + "</a>";
		html += '<sup>' + tm.Tm_Div[i] + '</sup><sub>#' + tm.Div_Rank[i] + '</sub>';
		html += ' ' + tm.Q_Loc[i] + ' #' + tm.Opp_Rank[i];
		html += " <a href=https://sites.google.com/site/swaermodel/teams/" + tm.Opp_Code[i] + ">" + tm.Opp[i] + "</a>";
		html += '<sup>' + tm.Q_Opp_Div[i] + '</sup><sub>#' + tm.Opp_Div_Rank[i] + '</sub></td>';
		html += '</tr>';
		html += '<tr>';
		if (tm.spread[i]>=0) {
		html += '<td style="text-align:right;">' + tm.Tm[i] + ' by</td><td style="text-align:left;">' + tm.spread[i] + '</td>';
		} else {
		html += '<td style="text-align:right;">' + tm.Opp[i] + ' by</td><td style="text-align:left;">' + tm.spread[i] * -1 + '</td>';
		}
		html += '<td>' + tm.Q_score[i] + '</td>';
		html += '</tr>';
	}	
	html += '</tbody></table>';	
	document.getElementById('rank-tbl').innerHTML = html;
	gadgets.window.adjustHeight();
  };


  function shape_map(div,yr) {
    var url = 'https://sites.google.com/site/swaermodel/map%20shapes.json?revision=3'
    var params = {};  
    params[gadgets.io.RequestParameters.CONTENT_TYPE] = gadgets.io.ContentType.JSON;
    gadgets.io.makeRequest(url, function(obj) {shape_resp(obj,div,yr);}, params);
  };



  function shape_resp(obj,div_sel,yr_sel) {
    	               
	var responseText = obj.data;
	var html = '<svg height="500" width="500">'

	for (var j in responseText[yr_sel][div_sel]) {
		html += '<polygon points= "' + responseText[yr_sel][div_sel][j].shp + '" style="fill:red;position:absolute;fill-rule:nonzero;" fill-opacity="0.5" onclick="MyFilter(\'' + j + '\',17)" />'
		html += '<text x="' + responseText[yr_sel][div_sel][j].center_x + '" y="' + responseText[yr_sel][div_sel][j].center_y + '" style="fill:white;font-size:30px;">' + j +'</text>'
	}
	html += '</svg>';

	document.getElementById('region').innerHTML = html;
  };




